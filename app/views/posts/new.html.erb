<div class="wr">
      <h2>投稿する</h2>

  <div class="enclosure">
    <%= form_with(model: @post, method: :post, local: true) do |f| %>
    <%= render "shared/error_messages", model: f.object %>

<%= f.file_field :images, name: 'post[images][]', id: 'post_image' %>
   <div id="image-list"></div>

<% @post.images.each do |image| %>
  <%= image_tag image, class: 'images' %>
<% end %>

    <div class="field">
      <%= f.label :title %><br/>
      <%= f.text_area :title, placeholder:"TITLE", maxlength:"40" %>
    </div>

    <div class="field">
      <%= f.label :text %><br/>
      <%= f.text_area :text, placeholder: "TEXT", rows: "10" %>
    </div>

    <div class="actions">
      <%= f.submit "SEND" %>
    </div>
    <% end %>
  </div>
</div>